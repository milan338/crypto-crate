{
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "arrowParens": "always",
    "useTabs": false,
    "tabWidth": 4,
    "printWidth": 100,
    "semi": true,
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf",
    "embeddedLanguageFormatting": "auto",
    "overrides": [
        {
            "files": "*.html",
            "options": {
                "singleQuote": false
            }
        }
    ]
}
